{
	"name": "<%= appName %>",
	"version": "<%= pkgVersion %>",
	"dependencies": {
		"angular": "~1.3.6",
	    "angular-bootstrap": "~0.12.0",
	    "jquery": "~2.1.1",<% if(filters.fontAwesome) { %>
	    "components-font-awesome": "~4.2.0",<% } %><% if(filters.bootstrap) { %>
	    "bootstrap": "~3.3.1",<% } %><% if(filters.bootstrapSass) { %>
	    "bootstrap-sass-official": "~3.1.1",<% } %><% if(filters.ngRoute) { %>
	    "angular-route": "~1.3.6",<% } %>
	    "json3": "~3.3.2",<% if(filters.ngResource) { %>
	    "angular-resource": "~1.2.*",<% } %><% if(filters.ngCookies) { %>
	    "angular-cookies": ">=1.2.*",<% } %><% if(filters.ngSanitize) { %>
	    "angular-sanitize": ">=1.2.*",<% } %><% if(filters.lodash) { %>
	    "lodash": "~2.4.1",<% } %><% if(filters.uiRouter) { %>
	    "angular-ui-router": "~0.2.10",<% } %><% if(filters.ngAnimate) { %>
	    "angular-animate": "~1.2.0",<% } %><% if(filters.ngTouch) { %>
    	"angular-touch": "~1.2.0",<% } %>
	    "es5-shim": "~4.0.5"
	},
	  	"devDependencies": {
	    "angular-mocks": "~1.2.0",
	    "angular-scenario": "~1.2.0"
  	}
}
